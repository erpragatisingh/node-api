    "paths": {
        "/api/getAllData": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get all data in system",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/api/addNewData": {
            "post": {
                "tags": [
                    "Users"
                ],
                "parameters": [
                    {
                        "name": "user",
                        "in": "body",
                        "description": "User that we want to create",
                        "schema": {
                            "required": [
                                "name",
                                "email",
                                "password"
                            ],
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                }
                            },
                            "companies": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "summary": "addNewData all data in system",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    }
